    "lint:md": "markdownlint --ignore-path ./.gitignore .",
    "lint": "npm-run-all --parallel --continue-on-error 'lint:*'",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "preview:storybook": "serve ./storybook-static --listen 5000",
    "test:e2e": "playwright test --workers=1",
    "test": "NODE_OPTIONS=--experimental-vm-modules yarn jest --config=jest.config.cjs",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules yarn jest --config=jest.config.cjs --collectCoverage,",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules yarn jest --config=jest.config.cjs --coverage=false --watch",
